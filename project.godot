config_version=5

[Fmod]

General/auto_initialize=true
General/channel_count=1024
General/is_live_update_enabled=true
General/is_memory_tracking_enabled=true
"Software Format/sample_rate"=48000
"Software Format/speaker_mode"=3
"Software Format/raw_speaker_count"=0
General/default_listener_count=8
General/should_load_by_name=false
DSP/dsp_buffer_size=512
DSP/dsp_buffer_count=4
"3D Settings/doppler_scale"=1.0
"3D Settings/distance_factor"=32.0
"3D Settings/rolloff_scale"=1.0
General/banks_path="res://assets/audio/Build/Desktop"

[application]

config/name="RiggedTest"
run/main_scene="res://scenes/menus/menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameState="*res://scripts/game_state.gd"
SteamGlobal="*res://scripts/steam_integration/steam_global.gd"
SteamInputGlobal="*res://scripts/input/steam_input_global.gd"
SteamLobbyGlobal="*res://scripts/multiplayer/steam_lobby_global.gd"
FmodManager="*res://addons/fmod/FmodManager.gd"
MasterBank="*res://scenes/utilities/master_bank.tscn"
SoundHandler="*res://scripts/globals/sound_handler.gd"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/borderless=true

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/fmod/plugin.cfg")

[gui]

theme/custom="res://main_theme.tres"

